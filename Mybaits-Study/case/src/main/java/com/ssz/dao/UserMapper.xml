<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--通过namespace去绑定一个对应的Dao/mapper接口-->
<mapper namespace="com.ssz.dao.UserDao">
   <!--select查询语句-->
    <select id="getUserList" resultType="com.ssz.pojo.User">
        select * from mybaits.user;
    </select>
</mapper>